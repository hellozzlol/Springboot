<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.team.prj.attend.service.mapper.attendMapper">

	<select id="attendSelectList" resultType="com.team.prj.attend.service.attendVO">
		SELECT AD_NO,
			   DT,
			   START_DT,
			   END_DT,
			   STATE
		FROM ATTEND
		WHERE AD_NO = #{adNo}
	</select>





</mapper>